#include <stdio.h>
#include <math.h>
int main() {
    int a,b,c,d,e,f;
    printf("Enter the marks of 5 subjects: \n ");
    scanf("%d%d%d%d%d",&a,&b,&c,&d,&e);
    printf("Math:%d \n",a);
    printf("Eng:%d \n",b);
    printf("SST:%d \n",c);
    printf("Science:%d \n",d);
    printf("Hindi:%d \n",e);
   f=a+b+c+d+e;
    printf("Sum of 5 subjects:%d \n",f);
     printf("Percentage:%f",(f/500.0)*100);
    return 0;
}
